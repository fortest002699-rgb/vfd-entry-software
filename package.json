{
  "name": "dts-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.13.4",
    "firebase": "^10.7.0",
    "googleapis": "^171.4.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.1.0",
    "pdfjs-dist": "^5.4.624",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.13.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.8.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:web": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "firebase:deploy": "npm run build && firebase deploy",
    "firebase:functions": "cd firebase-functions && npm install && npm run deploy",
    "deploy:all": "npm run firebase:deploy && npm run firebase:functions",
    "electron:dev": "npm run build:web && electron .",
    "electron:build": "npm run build:web && electron-builder",
    "cap:copy": "npx cap copy",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios"
  },
  "homepage": ".",
  "main": "electron/main.js",
  "build": {
    "appId": "com.dastechnoservices.dts",
    "productName": "DTS",
    "files": [
      "build/**/*",
      "backend/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "electron": "^25.9.8",
    "electron-builder": "^23.6.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
